pipeline {
   agent (label: jdk8)
   stages {
        stage('Git Checkout'){
           git branch: 'dev', url:'https://github.com/Reddy999eee/game-of-life.git'
            }
         stage ('Build'){
         sh 'mvn clean packge'
            }
         stage ('Archive & Test Results'){
        junit: '**/surefire-reports/*.xml'
       archiveArtifacts artifacts: '**/*.var' followSymlinks:false
}
}

}
