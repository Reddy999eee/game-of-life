pipeline{
    agent (label 'jdk8')
    stages{
        stage ('Git Checkout'){
            steps {
                git beanch: 'dev', url:'https://github.com/Reddy999eee/game-of-life.git'
            }
        }
        stage('Build'){
            steps {
                sh 'mvn clean package'
            }
        }
        stage ('Archeive & Test Results'){
            steps {
                junit: '**/surefire-reports/*.xml'
                archiveArtifacts artifacts: '**/*.war' followSymlinks:false
            }
        }
    }
}
